{
  "name": "grout",
  "version": "0.0.1",
  "description": "Geo tile server",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "prod": "node dist/server.js",
    "test": "vitest"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "data"
    ],
    "exec": "tsc && node ./dist/server.js",
    "ext": "ts,js,json"
  },
  "dependencies": {
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.3",
    "memfs": "^4.14.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  }
}
